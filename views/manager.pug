html
  include header.pug
  body(style="padding:16px;")
    br
    .row
      .col.s10.offset-s1.center-align
        a(id="calc_match_button",class="next waves-effect waves-light btn-large light-blue darken-1",onclick="calcMatch()")="Calculate score by match"
    .row
      .col.s6.offset-s3.center-align.input-field
          input#cal_by_match.validate(type='text')
          label(for='cal_by_match') Match No
    .row
      .col.s10.offset-s1.center-align
        a(id="calc_all_button",class="next waves-effect waves-light btn-large light-blue darken-1",onclick="calcAll()")="Calculate score all"
    .row
      .col.s10.offset-s1.center-align
        a(id="sum_score_button",class="next waves-effect waves-light btn-large light-blue darken-1",onclick="sumScore()")="Summarize score all"
        
    script(src="js/manager.js",type="text/javascript")
    
    


